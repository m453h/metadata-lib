    window.s.eVar73 = globalTracking.primaryTaxonomyBehindPaywall;
    window.s.eVar75 = globalTracking.contentCreatorBehindPaywall;
  };

  var googleAnalyticsUTM = function googleAnalyticsUTM() {
    var utmSource = queryParams.utm_source,
        utmMedium = queryParams.utm_medium,
        utmCampaign = queryParams.utm_campaign;
    var utmArray = [utmSource, utmMedium, utmCampaign];
