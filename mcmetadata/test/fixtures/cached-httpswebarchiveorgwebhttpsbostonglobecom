  window.formatPageLayout = function formatPageLayout(_ref) {
  var layout = _ref.layout,
      loggedIn = _ref.loggedIn;
  var pageLayout = '';

  if (layout === 'ArticleRightRail') {
    pageLayout = 'Rail';
  } else if (layout === 'PageRailLess') {
    pageLayout = 'Railless';
  } else {
    pageLayout = layout;
  }

