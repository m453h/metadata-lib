        <script type="text/javascript">
        function username_available(){
            if( $("#cyber_nm").val() ){
                $.ajax({
                    type: 'POST',
                    url: 'verificar_disponibilidad',
                    data:   'cyber_nm='+$("#cyber_nm").val(),
                    success: function(response) {
                        if( response==0 ){//DISPONIBLE
                            $('#avail_status').removeClass().addClass("disponible").html("Disponible").css('display','inline');
                            $('#user_avalability').val("Y");
                        }else{//NO DISPONIBLE
                            $('#avail_status').removeClass().addClass("no_disponible").html("No disponible").css('display','inline');
                            $('#user_avalability').val("N");
                        }
                    }
                });
            }
        }
    </script>
