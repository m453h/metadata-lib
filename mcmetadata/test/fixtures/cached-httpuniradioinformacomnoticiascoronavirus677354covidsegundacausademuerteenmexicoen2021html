<script type="text/javascript">
	$(function() {
		var $page =1;
		var $page_requested=0;
		var $prev_pos=0;
		var $excluded = "";
		var mn=true;
		var mv=false;
		var idx=0;		
		var previdx=-1;
				var secciones=['tijuana','policiaca','bajacalifornia','sandiego','estadosunidos','curioso','negocios','politica','tecnologia','salud','cultura'];
		$(window).scroll(function() {
			$woffset = $(document).height() - (768*2);
			$scrolled = $(window).height() + $(window).scrollTop();

			if($scrolled >= $woffset && $scrolled > $prev_pos && mn==true){
				if ($page_requested < $page ){
					$page_requested = $page
					$.getJSON('/noticias/coronavirus/'+ $page, { excluded: $excluded, lastday:1} )
					.done(function($res){
						if ($res.code==200){
							$('#endless_scroll').show();
							$('#endless_scroll #masde .content').append($res.data);
							$page++;
							_gaq.push(['_trackPageview','/noticias/coronavirus']);
						}
						else{
							$('#taboola').show();
							window._taboola = window._taboola || [];
							_taboola.push({
								mode:        'thumbnails-a',
								container:   'taboola-below-article-thumbnails',
								placement:   'Below Article Thumbnails',
								target_type: 'mix'
							});		

							window._taboola = window._taboola || [];
							_taboola.push({article:'auto'});
							!function (e, f, u) {
								e.async = 1;
								e.src = u;
								f.parentNode.insertBefore(e, f);
							}(document.createElement('script'), document.getElementsByTagName('script')[0], 'https://cdn.taboola.com/libtrc/uniradio/loader.js');

							mn=false;
							mv=true;
						}
					});
				}
			}
			$prev_pos = $scrolled;

			if($scrolled >= $woffset && mv==true){
				if (idx < secciones.length && previdx<idx){
					previdx=idx;
					$.getJSON('/noticias/masvistas/'+secciones[idx]+'/'+idx )
					.done(function(res){
						idx++;
						if (res.code==200){
							$('#endless_scroll').show().append(res.data);
							_gaq.push(['_trackPageview','/noticias/'+secciones[idx-1]]);
						}
					});
				}
			}
		});
	});
	
</script>
