    <script type="text/javascript">
        $(document).ready(function(){
            $('#playerutv').on('click',function(e){
                window.open("/player/utv","","width=720,height=580");
            });

            $('#close').on('click',function(e){
                $('#breaking-news').slideUp();
            });

            $('.btnBuscar').on('click',function(e){
                e.preventDefault();
                $('#submenu').hide();
                $('#searchbox').slideToggle('fast');
                $('.more a').removeClass('active');
                $('.btnMas i').removeClass('fa-caret-up');
                $('input[name=keyword]').focus();
            });

            $('.btnMas').on('click',function(e){
                e.preventDefault();
                $('#searchbox').hide();
                $('#submenu').slideToggle('fast');
                $('.more a').toggleClass('active');
                $(this).find('i').toggleClass('fa-caret-up');
            });

            $('.btnDashboard').on('click',function(e){
                e.preventDefault();
                $('#dashboard').slideToggle('fast');
                $('#submenu').hide();
                $('#searchbox').hide();

                $('.more a').removeClass('active');
                $('.btnMas i').removeClass('fa-caret-up');
            });

            $('#dashboard, #dashboard .close').on('click',function(e){
                $('#dashboard').hide();
            });

            $('#garita .tab-nav a').on('click',function(e){
                e.preventDefault();
                var cont = $(this).attr('data');
                $('#garita .tab-nav a').removeClass('active');
                $(this).addClass('active');
                $('#garita .tab-wrap .active').hide().removeClass('active');
                $(cont).show().addClass('active');
            });

            $('#masvistas .tab-nav a').on('click',function(e){
                e.preventDefault();
                var cont = $(this).attr('data');
                $('#masvistas .tab-nav a').removeClass('active');
                $(this).addClass('active');
                $('#masvistas .tab-wrap .active').hide().removeClass('active');
                $(cont).show().addClass('active');
            });


            var handler_click = 'mouseenter';

            $('.share').on(handler_click,function(e){
                var cont=$(this).attr('data');
                $(cont).show();
            });


            $('.slc').on('mouseleave',function(e){
                var cont=$(this).find('.share').attr('data');
                $(cont).hide();
            });

            var nav_offset = $('nav').offset().top;
            var stream_offset=0;
            if ( $('#streaming').length ){
                stream_offset = $('#streaming').offset().top+80;
            }
            $(window).scroll( function(){
                if ($(this).scrollTop() > nav_offset ){
                    $('nav').addClass('sticky');
                }
                else{
                    $('nav').removeClass('sticky');
                }
                if (stream_offset>0){
                    if ( $(this).scrollTop() > stream_offset ){
                        if ( $('#streaming').hasClass('stream_closed') ){

                        }
                        else{
                            $('#streaming').addClass('stream_sticky');
                        }
                    }
                    else{
                        $('#streaming').removeClass('stream_sticky');
                        $('#streaming').removeClass('stream_closed');
                    }
                }
            });

            $('#streaming').on('click',function(e){
                $('#streaming').removeClass('stream_sticky');
                $('#streaming').addClass('stream_closed');
            });

            if ( $( ".sharethis" ).length ){
                var st_offset = $('.sharethis').offset().top-50;
                var tb_offset = $('#comments').offset().top+250;
                $(window).scroll(function() {
                    if (($(this).scrollTop() > st_offset) && ($(this).scrollTop() < tb_offset) ){
                        $('.sharethis').addClass('st_vertical');
                    }
                    else{
                        $('.sharethis').removeClass('st_vertical');
                    }
                });
            }

        });

    </script>
