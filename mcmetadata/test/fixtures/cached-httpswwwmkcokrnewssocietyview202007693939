<!-- End Google Tag Manager (noscript) --><script>
var mkScrollNewsInfo = {
	params: "news_no=693939&news_year=2020&news_category=MK100801&author_id=477&pub_ymdhis=20200707104242&mk_pubdate=20200707&mk_panid=&mk_section=&mk_page=0&keyword=%C4%DA%B7%CE%B3%AA19%5E%BD%C5%B1%D4+%C8%AE%C1%F8",
	domain: document.domain, // 도메인
	uripath: document.location.pathname, // 페이지 URI
	access_time: new Date(), // 접근시간
	view_time: 0, // 머문 시간
	read_time: 0, // 기사 읽은 시간
	readfinish: "N", // 완독여부
	callBackTime: 100,
	readerLocation: 150,
	scrolltimer: 0,
	scroller: false,
	endContent: false,
	scrollStart: 0,
	trackLocation: function() { // 스크롤 이벤트
		var scroll_bottom = window.innerHeight + window.pageYOffset;
		var height = document.body.scrollHeight;
		var readElement;
		var stand = 0;

		// 스크롤시작(=읽기시작)
        if(scroll_bottom > this.readerLocation && !this.scroller) {
			var currentTime = new Date();
			this.scrollStart = currentTime.getTime();
			this.scroller = true;
		}

        if("" !== "") { // end id 가 있을 경우
			readElement = document.getElementById("");
			stand = window.pageYOffset + readElement.getBoundingClientRect().top;
		} else {
			readElement = document.getElementsByClassName("art_txt")[0];
			stand = window.pageYOffset + readElement.getBoundingClientRect().top + readElement.offsetHeight;
		}

		// 기사 끝까지 내렸을 시
		if(scroll_bottom >= stand && !this.endContent) {
			currentTime = new Date();
			contentScrollEnd = currentTime.getTime();

			// 기사 끝까지 내렸을 때까지 걸린 시간
			var timeToContentEnd = ((contentScrollEnd - this.scrollStart) / 1000).toFixed(2);

			console.log(timeToContentEnd);

			this.endContent = true;
			this.read_time = timeToContentEnd; // 기사 읽은 시간
			this.readfinish = "Y"; // 기사 완독
		}
	},
	calViewTime: function() { // 페이지 머문 시간 연산
		var currentTime = new Date();
		this.view_time = ((currentTime.getTime() - this.access_time.getTime()) / 1000).toFixed(2);

		var accessYear = this.access_time.getFullYear();
		var accessMonth = this.access_time.getMonth()+1;
		var accessDate = this.access_time.getDate();
		var accessHours = this.access_time.getHours();
		var accessMinutes = this.access_time.getMinutes();
		var accessSeconds = this.access_time.getSeconds();
		
		if(accessMonth < 10) {
			accessMonth = "0"+accessMonth;
		}
		if(accessDate < 10) {
			accessDate = "0"+accessDate;
		}
		if(accessHours < 10) {
			accessHours = "0"+accessHours;
		}
		if(accessMinutes < 10) {
			accessMinutes = "0"+accessMinutes;
		}
		if(accessSeconds < 10) {
			accessSeconds = "0"+accessSeconds;
		}
		this.access_time = ""+accessYear+accessMonth+accessDate+accessHours+accessMinutes+accessSeconds;
	},
	configParam: function() { // 파라미터 정의
		var tmpDomain = document.location.href.split('/');
		if(tmpDomain.length > 2) {
			this.domain = tmpDomain[2];
		}
		
		this.params += "&domain="+this.domain+"&uripath="+this.uripath+"&access_time="+this.access_time+"&view_time="+this.view_time+"&read_time="+this.read_time+"&readfinish="+this.readfinish;
		console.log(this.params);
	}
}

// 스크롤 이벤트 추가
window.addEventListener('scroll', function() {
	if(mkScrollNewsInfo.scrolltimer) {
		clearTimeout(mkScrollNewsInfo.scrolltimer);
	}

	mkScrollNewsInfo.scrolltimer = setTimeout(mkScrollNewsInfo.trackLocation(), mkScrollNewsInfo.callBackTime);
});

// 이동시 실행되는 이벤트
window.addEventListener('beforeunload', function() {
	try {
		mkScrollNewsInfo.calViewTime();
		mkScrollNewsInfo.configParam();
		
		var img= new Image();
		img.src="https://www25.mk.co.kr:8444/news/scrolldata/?" + mkScrollNewsInfo.params;
	} catch (e) {}
});
window.addEventListener('pagehide', function() {
	try {
		mkScrollNewsInfo.calViewTime();
		mkScrollNewsInfo.configParam();
		
		var img= new Image();
		img.src="https://www25.mk.co.kr:8444/news/scrolldata/?" + mkScrollNewsInfo.params;
	} catch (e) {}
});
</script>
<iframe id='newslog2_iframe' frameborder='0' scrolling='no' width='0' height='0' style='position:absolute;visibility:hidden;top:0;left:0;'></iframe>
<script>
    (function () {
        var guid = function () {
            function s4() {return Math.floor((1 + Math.random()) * 0x10000).toString(16).substring(1);}

            return s4() + s4() + s4() + s4() + s4() + s4() + s4() + s4();
        }
        var getCookie = function (cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }
        var setCookie = function (cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + "; " + expires;
        }
        var params = "maeche=mk&login_maeche=mk&maeche_category=MK100801&ustr=2020%2F693939&author_id=477&keyword=%C4%DA%B7%CE%B3%AA19%5E%BD%C5%B1%D4+%C8%AE%C1%F8&pub_ymdhis=20200707104242&mk_pubdate=20200707&mk_panid=&mk_section=&mk_page=0";
        var referrer = '';
        var tmp = document.referrer.split('/');
        if (tmp.length > 2) {referrer = tmp[2];}
        var domain = '';
        tmp = document.location.href.split('/');
        if (tmp.length > 2) {domain = tmp[2];}
        var pathname = document.location.pathname;
        var login_id = getCookie("cUserID");
        var login_yn = 'y';
        if (login_id == "") {
            login_id = getCookie("tmpUserID");
            login_yn = 'n';
            if (login_id == "") {
                login_id = guid();
                setCookie("tmpUserID", login_id, 365 * 100);
            }
        }
        var dumDate = new Date();
        params += "&referrer=" + referrer + "&domain=" + domain + "&pathname=" + pathname + "&login_id=" + login_id + "&login_yn=" + login_yn + "&dummy=" + dumDate.getTime();
        document.getElementById("newslog2_iframe").src = "//www.mk.co.kr/news/inc/mkNewsLogIframe.php?" + params;
        console.log(params);
    })();
</script>

