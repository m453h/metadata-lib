    <script>
        function jsonp(uri) {
            return new Promise(function (resolve, reject) {
                var id = '_' + Math.round(10000 * Math.random());
                var callbackName = 'jsonp_callback_' + id;
                window[callbackName] = function (data) {
                    delete window[callbackName];
                    var ele = document.getElementById(id);
                    ele.parentNode.removeChild(ele);
                    resolve(data);
                }

                var src = uri + '&callback=' + callbackName;
                var script = document.createElement('script');
                script.src = src;
                script.id = id;
                script.addEventListener('error', reject);
                (document.getElementsByTagName('head')[0] || document.body || document.documentElement).appendChild(script)
            });
        }
    </script>

    <script>

        var url = 'https://news.mk.co.kr/v7/inc/inc_ga_code_v2.php?no=' + '693939' + '&year=' + '2020' + '&callback=?';
        var dimension = '', dimension2 = '', dimension3 = '', dimension4 = '', dimension5 = '', dimension6 = '';

        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }

        jsonp(url).then(function (data) {

            if (data.dimension != "none") {
                dimension = data.dimension;
            } else {
                dimension = '';
            }

            if (data.dimension2 != "none") {
                dimension2 = data.dimension2;
            } else {
                dimension2 = '';
            }

            if (data.dimension3 != "none") {
                dimension3 = data.dimension3;
            } else {
                dimension3 = '';
            }

            if (data.dimension4 != "none") {
                dimension4 = data.dimension4;
            } else {
                dimension4 = '';
            }

            if (data.dimension5 != "0") {
                dimension5 = parseInt(data.dimension5);
            } else {
                dimension5 = 0;
            }

            if (data.dimension6 != "0") {
                dimension6 = parseInt(data.dimension6);
            } else {
                dimension6 = 0;
            }

            /*
                    console.log('## dimension :' + dimension);
                    console.log('## dimension2 :' + dimension2);
                    console.log('## dimension3 :' + dimension3);
                    console.log('## dimension4 :' + dimension4);
                    console.log('## dimension5 :' + dimension5);
                    console.log('## dimension6 :' + dimension6);
            */

            gtag('js', new Date());
            gtag('config', 'UA-24472307-90', {
                'custom_map': {
                    'dimension1': 'category',
                    'dimension2': 'code',
                    'dimension3': 'author',
                    'dimension4': 'date',
                    'dimension5': 'image',
                    'dimension6': 'length'
                },
                'linker': {
                    'domains': ['mk.co.kr', 'news.mk.co.kr', 'opinion.mk.co.kr', 'premium.mk.co.kr', 'star.mk.co.kr', 'sports.mk.co.kr', 'game.mk.co.kr', 'pulsenews.co.kr', 'uberin.mk.co.kr', 'mkbn.mk.co.kr', 'raythep.mk.co.kr',
                        'vip.mk.co.kr', 'estate.mk.co.kr', 'mplay.mk.co.kr', 'm.mk.co.kr', 'm.pulsenews.co.kr', 'm.raythep.com', 'raythe.mk.co.kr', 'photos.mk.co.kr', 'www.mk.co.kr']
                }
            });
            gtag('event', 'news_category', {
                'category': dimension,
                'code': dimension2,
                'author': dimension3,
                'date': dimension4,
                'image': dimension5,
                'length': dimension6,
                'non_interaction': true
            });
        });

    </script>

    <!-- 20181119 통합 트래픽 GA코드 추가  --><!-- 20211231 GA code - by magic204 -->
<script>
    var getCookie = function (name) {
        var value = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
        return value ? value[2] : null;
    };

    var setGtm = function (data) {
        const cSequenceID = getCookie('cSequenceID');
        const idxQ = document.location.href.indexOf('?');

        const output = {
            // userData
            'uid': (cSequenceID != null ? cSequenceID : ''),
            'loginType': (cSequenceID != null ? 'mk_login' : ''),
            'loginStatus': (cSequenceID != null ? 'logged In' : 'logged Out'),
            // pageData
            'userAgent': navigator.userAgent,
            'hostName': window.location.hostname,
            'pageUrl': document.location.href,
            'queryString': (idxQ >= 0 ? document.location.href.substring(idxQ) : ''),
            'referrer': document.referrer
        };

        // contentData
        if (data != null) {
            Object.entries(data).forEach(([key, value]) => {
                output[key] = value;
            });
        }
        console.log(output);
        window.dataLayer = window.dataLayer || [];
        dataLayer.push(output);

        console.log(dataLayer);
    }
        function jsonp(uri) {
        var id = 'mk_' + Math.round(10000 * Math.random());
        var callbackName = 'jsonp_callback_' + id;
        window[callbackName] = function (data) {
            delete window[callbackName];
            var ele = document.getElementById(id);
            ele.parentNode.removeChild(ele);
            setGtm(data);
        }

        var src = uri + '&callback=' + callbackName;
        var script = document.createElement('script');
        script.src = src;
        script.id = id;
        script.addEventListener('error', setGtm);
        (document.getElementsByTagName('head')[0] || document.body || document.documentElement).appendChild(script);
    }

    var url = 'https://news.mk.co.kr/v7/inc/inc_ga_code_v3.php?no=693939&year=2020';
    jsonp(url);
    </script>
