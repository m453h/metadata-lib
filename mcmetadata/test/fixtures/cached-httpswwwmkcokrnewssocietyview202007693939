						<!--// 시선집중  끝 -->    <script type="text/javascript">
        function getLivereCnt(mcp_url){
            var xhr = new XMLHttpRequest();
            var formData = new FormData();
            formData.append('mcp_url',mcp_url);
            xhr.open('POST', 'https://m.mk.co.kr/origin/news/ajax/livere_cnt_ajax.php',true);
            xhr.timeout = 3000;
            xhr.onload = function() {
                if (xhr.status === 200 || xhr.status === 201) {
                    var data=xhr.responseText;
                    var rElement=document.getElementById('reply_btn_text');
                    if(rElement){
                        rElement.innerHTML=data;
                    }
                }
            };
            xhr.ontimeout = function (e) {
                var rElement=document.getElementById('reply_btn_text');
                if(rElement){
                    rElement.innerHTML="0";
                }
            };
            xhr.send(formData); // 폼 데이터 객체 전송
        }
        getLivereCnt('news.mk.co.kr/newsRead.php?year=2020&no=693939');
    </script>

